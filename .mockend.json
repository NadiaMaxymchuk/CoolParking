{
  "Balance": {
    "balance": { 
      "regexp": "[1-9]{2,28}\\.[0-9]{1,28}m" 
    }
  },
  "Comment": {
    "balance": {
      "belongsTo": "Balance"
    },
    "vehicleType": {
      "int": [1, 2, 3, 4] 
    },
    "id": {
      "regexp": "([A-Z][A-Z]\\-[0-9][0-9][0-9][0-9]\\-[A-Z][A-Z])"
    }
  }
}
